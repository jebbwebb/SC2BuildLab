<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/main.css" />
    <title>Document</title>
  </head>
  <body>
    <body>
        <%- include('navbar.ejs') %>

            <main>
                <% if (builds.length > 0) { %>
                    <div class="builds container">
                        <% for (let build of builds) { %>
                            <div class="builds">
                                <div class="build">
                                  <a href="/admin/view-build/<%= build.postId %>"><%= build.title %></a>
                                  <div class="build-info">
                                    <p>
                                        <%= build.race %>v<%= build.enemy %> 
                                        
                                        
                                        <p class="created_by">
                                  
                                          <% if (build.user) { %>
                                           Created by:   <%= build.user.username %>
                                            <% } %>
                                      </p>
                                      
                                      </p>
                                    </div>
                                    <div class="build-type">
                                      <p class="build-type"><%= build.type %> </p>
                                    </div>
                                </div>
                               
                                </div>
                                <div class="no-builds">
                                  <% } %>
                                
                              <% } else { %>
                                <h1>No Builds Found!</h1>
                              <% } %>
                            </div>
                          </main>
                          <script src="/css/script.js"></script>
                        </body>
                        </html>
                                    
                               
                              
                                   
                                   
                                
                              
                                
                                
                                
                                  
                                
                                
           
            
  

